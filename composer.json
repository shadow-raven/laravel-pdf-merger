{
    "name": "shadow-raven/laravel-pdf-merger",
    "type": "library",
    "description": "Package for Laravel that merges multiple PDFs into one.",
    "keywords": [
      "shadow-raven",
      "laravel",
      "pdf",
      "merger",
      "pdfmerger",
      "pdf-merger"
    ],
    "license": "MIT",
    "authors": [
      {
        "name": "ShadowRaven"
      }
    ],
    "require": {
      "illuminate/support": ">=5.5",
      "setasign/fpdi-fpdf": ">=2.0"
    },
    "autoload": {
      "psr-4": {
          "ShadowRaven\\LaravelPDFMerger\\Facades\\": "vendor/ShadowRaven/laravel-pdf-merger/src/Facades/",
          "ShadowRaven\\LaravelPDFMerger\\Providers\\": "vendor/ShadowRaven/laravel-pdf-merger/src/Providers/",
          "ShadowRaven\\LaravelPDFMerger\\": "src"
      }
    },
    "minimum-stability": "stable"
}
